{
  "name": "solidjs-tailwind",
  "version": "0.1.0",
  "private": true,
  "description": "SolidJs and TailwindCSS",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .js,.jsx ./src",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write ./src --ignore-path .gitignore",
    "storybook": "start-storybook"
  },
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-backgrounds": "6.5.13",
    "@storybook/addon-docs": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/addon-measure": "6.5.13",
    "@storybook/addon-outline": "6.5.13",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/client-api": "6.5.13",
    "@storybook/client-logger": "6.5.13",
    "@storybook/core-common": "6.5.13",
    "@storybook/html": "6.5.13",
    "@testing-library/jest-dom": "5.16.5",
    "@typescript-eslint/parser": "5.41.0",
    "autoprefixer": "10.4.12",
    "eslint": "8.26.0",
    "eslint-plugin-solid": "0.7.4",
    "eslint-plugin-unused-imports": "2.0.0",
    "jsdom": "20.0.1",
    "msw": "^0.49.1",
    "msw-storybook-addon": "^1.6.3",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "solid-testing-library": "0.3.0",
    "storybook-addon-mock": "3.2.0",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-plugin-solid": "2.3.10",
    "vitest": "0.24.3"
  },
  "dependencies": {
    "@heroicons/react": "1.0.5",
    "@solidjs/router": "0.5.0",
    "@tailwindcss/typography": "^0.5.8",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "dayjs": "^1.11.6",
    "highlight.js": "^11.7.0",
    "isomorphic-fetch": "^3.0.0",
    "prism-react-renderer": "^1.3.5",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "solid-heroicons": "^3.1.1",
    "solid-icons": "^1.0.2",
    "solid-highlight": "^0.1.12",
    "solid-js": "1.6.0",
    "solid-markdown": "1.1.0",
    "tailwindcss": "3.2.1"
  },
  "keywords": [
    "solidjs",
    "tailwind"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "webpack",
        "@storybook/*"
      ]
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
